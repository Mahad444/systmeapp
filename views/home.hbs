<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th class="scope">#</th>
      <th class="scope">First Name</th>
      <th class="scope">Last Name</th>
      <th class="scope">Email</th>
      <th class="scope">PhoneNumber</th>
      <th class="text-end">Action</th>
    </tr>
  </thead>
  <tbody>
    <tr>
    {{#each rows}}
      <th scope="row">{{this.id}}</th>
      <td>{{this.FirstName}}</td>
      <td>{{this.LastName}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td class="text-end">
        <a href="#" class="btn btn-light btn-small" type="button"><i class="bi bi-eye"></i> View</a>
        <a href="#" class="btn btn-light btn-small" type="button"><i class="bi bi-pencil"></i> Edit</a>
        <a href="#" class="btn btn-light btn-small" type="button"><i class="bi bi-person-x"></i> Delete</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>